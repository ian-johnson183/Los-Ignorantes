{UTF-8}
Abandono de pasajeros = WITH LOOKUP (
	Saturación,
		  ([(0,0)-(10,10)],(0,0),(25,5),(50,20),(75,50),(100,200) ))
	~	personas/Year
	~		|

Ascenso de pasajeros=
	MIN( Pasajeros , Capacidad de un colectivo*Colectivos )
	~	personas/Year
	~		|

Llegada de pasajeros=
	Tendencia a usar transporte público
	~	personas/Year
	~		|

Saturación=
	100*Pasajeros/Capacidad de la parada
	~	
	~		|

Beneficio=
	Beneficio anual
	~	$/Year
	~		|

Beneficio anual=
	Ingresos-Costos
	~	$/Year
	~		|

Cantidad inicial de colectivos=
	2
	~	colectivos [0,10,1]
	~		|

Dinero para nuevas unidades=
	Fracción invertida*Presupuesto
	~	$/Year
	~		|

Capacidad de un colectivo=
	40
	~	personas/colectivos [0,50,2]
	~		|

Compra de nuevas unidades=
	Dinero para nuevas unidades/Precio de un colectivo
	~	colectivos/Year
	~		|

Costo operativo=
	230
	~	$/(colectivos*Year) [0,1000,10]
	~		|

Costos=
	Colectivos*Costo operativo
	~	$/Year
	~		|

Fracción invertida=
	0.01
	~	1 /Year [0,0.1,0.01]
	~		|

Ingresos=
	Precio del boleto*Ascenso de pasajeros
	~	$/Year
	~		|

Colectivos= INTEG (
	Compra de nuevas unidades,
		Cantidad inicial de colectivos)
	~	colectivos
	~		|

Ocupación=
	100*(Ascenso de pasajeros)/(Capacidad de un colectivo*Colectivos)
	~	 [0,1]
	~		|

Pasajeros= INTEG (
	Llegada de pasajeros-Abandono de pasajeros-Ascenso de pasajeros,
		0)
	~	personas
	~		|

Precio del boleto=
	70
	~	$/personas [0,200,2]
	~		|

Precio de un colectivo=
	1e+06
	~	$/colectivos [0,2e+06,100000]
	~		|

Presupuesto Inicial=
	5e+06
	~	$ [0,1e+07,100000]
	~		|

Presupuesto= INTEG (
	Beneficio-Dinero para nuevas unidades,
		Presupuesto Inicial)
	~	$
	~		|

Tendencia a usar transporte público = WITH LOOKUP (
	Precio del boleto,
		      ([(0,0)-(200,300)],(0,200),(200,0) ))
	~	personas/Year [0,100]
	~		|

Capacidad de la parada=
	120
	~	personas [0,150,5]
	~		|

********************************************************
	.Control
********************************************************~
		Simulation Control Parameters
	|

FINAL TIME  = 2030
	~	Year
	~	The final time for the simulation.
	|

INITIAL TIME  = 2020
	~	Year
	~	The initial time for the simulation.
	|

SAVEPER  = 
        TIME STEP
	~	Year [0,?]
	~	The frequency with which output is stored.
	|

TIME STEP  = 0.0025
	~	Year [0,?]
	~	The time step for the simulation.
	|

